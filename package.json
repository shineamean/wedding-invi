    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@types/navermaps": "^3.9.1",
    "photoswipe": "^5.4.4",
    "react-naver-maps": "^0.1.4"
  }
}
